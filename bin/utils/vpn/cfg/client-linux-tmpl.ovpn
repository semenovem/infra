client
dev tun
#proto tcp
#proto udp
#remote spb.evgio.com 443
resolv-retry infinite
nobind
user nobody
group nogroup
persist-key
persist-tun

remote-cert-tls server

cipher AES-256-GCM
;cipher AES-256-CBC
auth SHA256

#comp-lzo

verb 0
;mute 20

key-direction 1

script-security 2
up /etc/openvpn/update-resolv-conf
down /etc/openvpn/update-resolv-conf

auth-nocache


# remote-cert-tls server
